issn = {1476-4687},
journal = {Nature},
month = {jul},
number = {7409},
pages = {57--60},
pmid = {22832584},
title = {{Quantum nonlinear optics with single photons enabled by strongly interacting atoms.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22832584 http://www.nature.com/doifinder/10.1038/nature11361},
volume = {488},
year = {2012}
}
