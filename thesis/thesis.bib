doi = {10.1103/PhysRevA.24.2567},
file = {:Users/tommy/Documents/Mendeley Desktop/Konopnicki, Eberly/Physics Letters A/Konopnicki, Eberly - 1981 - Simultaneous propagation of short different-wavelength optical pulses.pdf:pdf},
