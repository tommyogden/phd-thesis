@article{Unanyan1998,
abstract = {We describe a method for creating an arbitrary coherent superposition$\backslash$nof two atomic states in a controlled and robust way by using a sequence$\backslash$nof three pulses in a four-state system. The proposed technique is$\backslash$nbased on the existence of two degenerate dark states (i.e. states$\backslash$nhaving no component of the excited state) and their interaction.$\backslash$nThe mixing of the dark states can be controlled by changing the relative$\backslash$ndelay of the pulses, and thus an arbitrary superposition state can$\backslash$nbe generated. It is shown that the method is robust against small$\backslash$nvariations of parameters (e.g. the area of the pulses) and is insensitive$\backslash$nto radiative decay from the intermediate excited state. A time reversed$\backslash$nversion of the technique makes possible the determination of phase$\backslash$noccurring in a superposition of two atomic states.},
